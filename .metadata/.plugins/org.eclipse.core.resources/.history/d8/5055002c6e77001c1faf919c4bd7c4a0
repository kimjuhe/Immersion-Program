package day1_5;

public class Caculator {
	private int memory;
	
	public int getMemory() {
		return memory;
	}
	
	public void setMemory(int Memory) {
		this.memory = memory;
		try {
			Thread.sleep(2000);
		}catch(InterruptedException e) {}
		System.out.println(Thread.currentThread().getName()+":"+this.memory);
	}
}
